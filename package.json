{
  "name": "kyh.io",
  "description": "Landing page to Kaiyu's website",
  "version": "1.1.0",
  "main": "src/index.html",
  "author": "Kaiyu Hsu",
  "license": "MIT",
  "homepage": "https://kyh.io",
  "repository": {
    "type": "git",
    "url": "https://tehkaiyu@github.com/tehkaiyu/tehkaiyu.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/tehkaiyu/tehkaiyu.github.io/issues"
  },
  "scripts": {
    "clean": "rimraf build",
    "deploy": "npm run build && npm run copy",
    "copy": "copyfiles -f build/* .",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html"
  },
  "eslintConfig": {
    "extends": ["airbnb", "plugin:prettier/recommended"],
    "env": {
      "browser": true,
      "node": true
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "devDependencies": {
    "eslint": "5.8.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "parcel-bundler": "^1.10.3",
    "prettier": "1.14.3"
  },
  "dependencies": {
    "animejs": "2.2.0",
    "intersection-observer": "0.5.1",
    "three": "0.97.0",
    "three-orbit-controls": "82.1.0",
    "tween.js": "16.6.0"
  }
}
