{
  "name": "kyh-monorepo",
  "private": true,
  "packageManager": "pnpm@10.28.1",
  "scripts": {
    "build": "turbo run build",
    "clean": "git clean -xdf node_modules",
    "clean-workspaces": "turbo run clean",
    "db-supabase": "pnpm -F db supabase",
    "dev": "turbo watch dev --continue",
    "dev-cli": "pnpm -F kyh dev",
    "dev-covid": "pnpm -F @repo/covid-19-dashboard dev",
    "dev-kwadrants": "pnpm -F @repo/kwadrants dev",
    "dev-kyh": "pnpm -F @repo/kyh dev",
    "dev-party": "pnpm -F @repo/party dev",
    "dev-tc": "pnpm -F @repo/tc dev",
    "dev-vis-ml": "pnpm -F @repo/vis-ml dev",
    "deploy-party": "pnpm -F @repo/party deploy",
    "format": "turbo run format --continue -- --cache --cache-location .cache/.prettiercache",
    "format-fix": "turbo run format --continue -- --write --cache --cache-location .cache/.prettiercache",
    "lint": "turbo run lint --continue -- --cache --cache-location .cache/.eslintcache",
    "lint-fix": "turbo run lint --continue -- --fix --cache --cache-location .cache/.eslintcache",
    "lint-ws": "pnpm dlx sherif@latest",
    "postinstall": "pnpm lint-ws",
    "typecheck": "turbo run typecheck",
    "changeset": "changeset",
    "publish-packages": "changeset version && changeset publish",
    "workspace-new": "turbo gen init"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@kyh/prettier-config": "workspace:^",
    "dotenv-cli": "^11.0.0",
    "prettier": "catalog:",
    "tsx": "^4.21.0",
    "turbo": "^2.7.5",
    "typescript": "catalog:"
  },
  "prettier": "@kyh/prettier-config"
}
