{
  "name": "kyh.io",
  "version": "1.5.0",
  "description": "Kaiyu's personal website",
  "homepage": "https://kyh.io",
  "bugs": {
    "url": "https://github.com/kyh/kyh.io/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://kyh@github.com/kyh/kyh.io.git"
  },
  "license": "MIT",
  "author": "Kaiyu Hsu",
  "main": "src/index.html",
  "scripts": {
    "build": "npm run clean && parcel build src/index.html",
    "clean": "rimraf dist",
    "dev": "parcel serve src/index.html --open",
    "format": "prettier --write src",
    "postinstall": "husky install"
  },
  "browser": "dist/index.html",
  "targets": {
    "browser": {
      "engines": {
        "browsers": [
          "last 2 versions",
          "not edge < 76",
          "not firefox < 67"
        ]
      }
    }
  },
  "dependencies": {
    "animejs": "^3.2.1",
    "lazysizes": "^5.3.0",
    "three": "^0.127.0",
    "tippy.js": "^6.2.7"
  },
  "devDependencies": {
    "@parcel/transformer-image": "^2.0.0-beta.2",
    "husky": "^6.0.0",
    "parcel": "next",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2"
  }
}
