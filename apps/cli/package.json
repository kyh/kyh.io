{
  "name": "kyh",
  "version": "0.1.2",
  "description": "Personal CLI tool",
  "type": "module",
  "bin": {
    "kyh": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun build src/index.tsx --outdir dist --target bun && echo '#!/usr/bin/env bun' | cat - dist/index.js > dist/temp && mv dist/temp dist/index.js",
    "dev": "bun run --watch src/index.tsx",
    "clean": "rm -rf dist node_modules",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore"
  },
  "dependencies": {
    "@opentui/core": "^0.1.83",
    "@opentui/react": "^0.1.83",
    "react": "catalog:"
  },
  "devDependencies": {
    "@kyh/eslint-config": "workspace:^",
    "@kyh/prettier-config": "workspace:^",
    "@kyh/tsconfig": "workspace:^",
    "@types/bun": "latest",
    "@types/react": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@kyh/prettier-config"
}
