{
  "extends": "@kyh/tsconfig/base.json",
  "compilerOptions": {
    "noEmit": false,
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "outDir": "dist",
    "rootDir": ".",
    "types": ["node"],
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "allowJs": false,
    "checkJs": false
  },
  "include": ["src/**/*.ts", "tests/**/*.ts"],
  "exclude": ["dist", "node_modules"]
}
